@model IEnumerable<SstDCSA.Models.tbl_DCSA2304_mark2>
@{
    ViewBag.Title = "ExportDCSA2304 Mark";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.0.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.5/css/dataTables.bootstrap5.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.5/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.2.2/css/buttons.dataTables.min.css">

</head>

<h2 class="text-center mt-3">DCSA 2304 Result Export</h2>

<hr />

<div class="container">
    <div class="row">
        <table id="DCSA1304" class="display" style="font-size:12px">
            <thead>
                <tr>
                    <th>
                        No
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tbl_student.st_registration)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tbl_student.st_name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.subj_code)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tma1)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tma2)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.total_tma)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.experiment)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.record_book)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.viva)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.total_practical)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.exam_term)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.st_center_code)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.submit_date)
                    </th>

                </tr>

            </thead>
            <tbody>
                @{ int i = 1; }
                @foreach (var item in Model)
                {
                    <tr>
                        <td scope="row">
                            @i
                        </td>
                        @{ i++; }
                        <td>
                            @Html.DisplayFor(modelItem => item.tbl_student.st_registration)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tbl_student.st_name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.subj_code)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tma1)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tma2)
                        </td>
                        <td class="bg-warning">
                            @Html.DisplayFor(modelItem => item.total_tma)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.experiment)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.record_book)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.viva)
                        </td>
                        <td class="bg-info">
                            @Html.DisplayFor(modelItem => item.total_practical)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.exam_term)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.st_center_code)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.submit_date)
                        </td>
                    </tr>
                }

            </tbody>

        </table>
    </div>
</div>

@section scripts{

    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js"></script>
    <script src=" https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src=" https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.print.min.js"></script>


    <script>
        $(document).ready(function () {
            $('#DCSA1304').DataTable({
                dom: 'Bfrtip',
                buttons: [
                    'csv', 'excel', 'pdf', 'print'
                ]
            });
        });

    </script>
}


